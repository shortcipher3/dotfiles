
# fill in with values
Host home
  Hostname 127.0.0.1
  User shortcipher3
  IdentityFile ~/.ssh/id_ed25519
  Port 22

Host iot
  Hostname 192.168.1.2
  User root
  ProxyCommand ssh -W %h:%p shortcipher3@127.0.0.1

Host *.iot
  User root
  ProxyCommand ssh -W $(basename %h .iot):%p shortcipher3@127.0.0.1

# for multiple users https://stackoverflow.com/questions/13103083/how-do-i-push-to-github-under-a-different-username
Host shortcipher3-github.com
  Hostname github.com
  Port 22
  User git
  IdentityFile ~/.ssh/id_ed25519

# %h hostname
# %p port
# %r remote user
